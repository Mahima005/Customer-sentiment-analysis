   "source": [
    "def sentiment(pol):\n",
    "    if pol>=0.75:\n",
    "     return \"Extremely positive\"\n",
    "    elif pol>=0:\n",
    "        return \"Positive\"\n",
    "    elif pol ==0:\n",
    "        return \"Neutral\"\n",
    "    elif pol <= -0.75:\n",
    "        return \"Negative\"\n",
    "    else:\n",
    "        return \"Extremply negative\"\n",
    "\n",
    "df[\"Sentiment\"] = df[\"Average_polarity\"].apply(sentiment)\n",
    "df"
   ]
  },
