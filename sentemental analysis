    "from textblob import blob\n",
    "def get_polarity(sentences):\n",
    "    return [TextBlob(sentence).sentiment.polarity for sentence in sentences]\n",
    "\n",
    "df[\"polarity\"] = df[\"Reviews_1\"].apply(get_polarity)\n",
    "df"
